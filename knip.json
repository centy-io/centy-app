{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.tsx"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "dist/**",
    "coverage/**",
    "src/gen/**",
    "src/types/**/*.d.ts"
  ],
  "ignoreDependencies": ["@bufbuild/protoc-gen-es", "@cspell/dict-he"],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
